        "ts-node": "^10.9.2",
        "tslib": "^2.8.1",
        "typescript": "^5.9.3",
        "vite": "^7.3.0",
        "vite-plugin-dts": "^4.5.4"
    },
    "peerDependencies": {
        "express-session": ">=1",
        "surrealdb": ">=2.0.0-alpha.14 <3.0.0"
