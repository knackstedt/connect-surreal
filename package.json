{
    "name": "connect-surreal",
    "version": "2.2.1",
    "description": "SurrealDB session store for Connect",
    "author": "Andrew G. Knackstedt",
    "license": "MIT",
    "type": "module",
    "main": "./dist/connect-surreal.cjs",
    "module": "./dist/connect-surreal.js",
    "types": "./dist/connect-surreal.d.ts",
    "repository": {
        "type": "git",
        "url": "https://github.com/knackstedt/connect-surreal"
